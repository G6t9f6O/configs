Xcursor.theme:  Vanilla-DMZ-AA

Xft.antialias:  1
Xft.hinting:    1
Xft.autohint:   0
Xft.hintstyle:  hintmedium
Xft.lcdfilter:  lcddefault
Xft.dpi:        120
Xft.rgba:       rgb

! behelit color theme
URxvt.background:    #101010
URxvt.foreground:    #60ff60
URxvt.cursorColor:   #60ff60

! black
URxvt.color0:        #1c1c1c
URxvt.color8:        #4d4d4d

! red
URxvt.color1:        #d81860
URxvt.color9:        #f00060

! green
URxvt.color2:        #60ff60
URxvt.color10:       #70ff70

! yellow
URxvt.color3:        #f9fd75
URxvt.color11:       #f9fd80

! blue
URxvt.color4:        #5d97fa
URxvt.color12:       #679dfa

! magenta
URxvt.color5:        #a78edb
URxvt.color13:       #ad97db

! cyan
URxvt.color6:        #4695c8
URxvt.color14:       #5a9dc8

! white
URxvt.color7:        #f3ebe2
URxvt.color15:       #eeeeee

! urxvt
URxvt.termName:             rxvt-unicode-256color
URxvt.geometry:             90x25
URxvt.reverseVideo:         false
URxvt.font:                 xft:Hermit:pixelsize=14
URxvt.scrollBar:            true
URxvt.scrollBar_floating:   true
URxvt.scrollBar_right:      false
URxvt.scrollColor:          #0c0c0c
URxvt.saveLines:            5500
URxvt.perl-ext-common:      default,matcher,-searchable-scrollback
URxvt.url-launcher:         chromium
URxvt.matcher.button:       1
URxvt.colorUL:              #5f87d7
URxvt.colorIT:              #d81860
URxvt.cursorBlink:          true
URxvt.urgentOnBell:         true

!! rofi
rofi.columns:                        2
rofi.bw:                             2
rofi.terminal:                       termite
rofi.font:                           hermit bold 14px
rofi.color-enabled:                  true
!                                       bg      fg    bgalt    hlbg    hlfg
rofi.color-normal:                   #101010,#404040,#101010,#101010,#5f87ff
rofi.color-urgent:                   #101010,#f00060,#101010,#101010,#5f87ff
rofi.color-active:                   #101010,#00ff77,#101010,#101010,#5f87ff
!                                       bg    border
rofi.color-window:                   #101010,#202020
