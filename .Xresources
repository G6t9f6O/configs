Xcursor.theme:  Vanilla-DMZ-AA

Xft.antialias:  1
Xft.hinting:    1
Xft.autohint:   0
Xft.hintstyle:  hintmedium
Xft.lcdfilter:  lcddefault
Xft.dpi:        120
Xft.rgba:       rgb

! behelit color theme
URxvt.background:    #101010
URxvt.foreground:    #60ff60
URxvt.cursorColor:   #60ff60

! black
URxvt.color0:        #1c1c1c
URxvt.color8:        #4d4d4d

! red
URxvt.color1:        #d81860
URxvt.color9:        #f00060

! green
URxvt.color2:        #60ff60
URxvt.color10:       #70ff70

! yellow
URxvt.color3:        #f9fd75
URxvt.color11:       #f9fd80

! blue
URxvt.color4:        #5d97fa
URxvt.color12:       #679dfa

! magenta
URxvt.color5:        #a78edb
URxvt.color13:       #b29fdb

! cyan
URxvt.color6:        #4695c8
URxvt.color14:       #5a9dc8

! white
URxvt.color7:        #f3ebe2
URxvt.color15:       #eeeeee

! urxvt
URxvt.termName:             rxvt-unicode-256color
URxvt.geometry:             90x25
URxvt.reverseVideo:         false
!!! these two go together !!!
!URxvt.font:                 xft:Source Code Pro:pixelsize=14
!URxvt.letterSpace:          -1
!!! these two go together !!!
URxvt.font:                 xft:Hermit:style=medium:pixelsize=13
URxvt.letterSpace:          1
URxvt.scrollBar:            true
URxvt.scrollBar_floating:   true
URxvt.scrollBar_right:      false
URxvt.scrollColor:          #0c0c0c
URxvt.saveLines:            5500
URxvt.perl-ext-common:      default,matcher,-searchable-scrollback,clipboard,keyboard-select
URxvt.url-launcher:         firefox
URxvt.matcher.button:       1
URxvt.colorUL:              #5f87d7
URxvt.colorIT:              #d81860
URxvt.cursorBlink:          true
URxvt.keysym.Mod4-Meta-c:   perl:clipboard:copy
URxvt.keysym.Mod4-Meta-v:   perl:clipboard:paste
URxvt.keysym.Mod4-C-v:      perl:clipboard:paste_escaped
URxvt.copyCommand:          xclip -i -selection clipboard
URxvt.pasteCommand:         xclip -o -selection clipboard
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.urgentOnBell:         true
! some distributions (like fedora) use shift-{up,down,home,end} for scrolling.
! the following settings will rebind them to their normal character sequence.
URxvt.keysym.S-Up:          \033[a
URxvt.keysym.S-Down:        \033[b
URxvt.keysym.S-Home:        \033[7$
URxvt.keysym.S-End:         \033[8$
!!! for transparent terminal uncomment the following
!!! also use feh to set your wallpaper
!URxvt.transparent:          true
!URxvt.shading:              15
!URxvt.inheritPixmap:        true
